{
  "name": "flexcms",
  "version": "1.0.0",
  "description": "web cms",
  "main": "server.js",
  "repository": "https://github.com/SatriaAPN/flexcms-web-blog-app.git",
  "author": "Satria APN <satrianusa10@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "nanoid": "^3.1.25",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "scripts": {
    "start-dev": "nodemon dist/api/server.js",
    "start-prod": "node dist/server.js",
    "build": "tsc -w",
    "install-views": "npm install views/",
    "db-create": "sequelize db:create",
    "db-migrate-status": "sequelize db:migrate:status",
    "db-migrate-up": "sequelize db:migrate",
    "db-migrate-down": "sequelize db:migrate:undo",
    "db-migrate-down-all": "sequelize db:migrate:undo:all",
    "db-migrate-redo": "npm run migrate-down && npm run migrate-up",
    "db-migrate-redo-all": "npm run migrate-down-all && npm run migrate-up"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
